{
  "name": "node-schedule",
  "version": "0.2.6",
  "description": "A cron-like and not-cron-like job scheduler for Node.",
  "keywords": [
    "schedule",
    "task",
    "job",
    "cron"
  ],
  "main": "./lib/schedule.js",
  "scripts": {
    "compile": "npm run clean && ./node_modules/.bin/babel src --out-dir lib",
    "prepublish": "npm run compile",
    "pretest": "npm run compile",
    "test": "./node_modules/.bin/nodeunit test",
    "lint": "./node_modules/.bin/eslint src",
    "clean": "rm -rf lib"
  },
  "author": {
    "name": "Matt Patenaude",
    "email": "matt@mattpatenaude.com",
    "url": "http://mattpatenaude.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/node-schedule/node-schedule.git"
  },
  "dependencies": {
    "cron-parser": "0.6.1",
    "long-timeout": "0.0.2"
  },
  "devDependencies": {
    "babel": "^5.4.7",
    "babel-eslint": "^3.1.10",
    "coveralls": "^2.11.2",
    "eslint": "^0.22.1",
    "istanbul": "^0.3.8",
    "nodeunit": "^0.9.1",
    "sinon": "^1.14.1"
  }
}
