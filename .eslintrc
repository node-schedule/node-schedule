{
  "parser": "babel-eslint",
  "env": {
    "node": true
  },
  "rules": {
    "eqeqeq": [2, "smart"],
    "indent": [2, 2],
    "no-constant-condition": false,
    "no-redeclare": 1,
    "no-underscore-dangle": false,
    "no-use-before-define": [true,"nofunc"],
    "quotes": [2, "single"],
    "space-before-blocks": 2,
    "strict": 2
  }
}
